{
    "name": "burst/eurofiber",
    "description": "Eurofiber Drupal 9 CMS",
    "type": "project",
    "authors": [
        {
            "name": "Burst",
            "email": "developers@burst-digital.com"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "git",
            "url": "https://github.com/hechoendrupal/drupal-console.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "burst/drupal-distribution": "^3.1",
        "drupal/graphql": "^4.0",
        "drupal/languagefield": "1.x-dev@dev",
        "drupal/redis": "^1.5",
        "drupal/replicate": "^1.0",
        "drupal/replicate_ui": "^1.0",
        "drupal/smtp": "^1.0"
    },
    "config": {
        "sort-packages": true,
        "discard-changes": true,
        "platform": {
            "php": "7.4.999",
            "ext-curl": "7.4.999",
            "ext-gd": "7.4.999"
        }
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$vendor}-{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "patches": {
            "drupal/core": {
                "#2429321 Verify that the configured service exists before calling it in CacheFactory": "https://www.drupal.org/files/issues/2020-05-04/2429321-9.1-75.patch"
            },
            "drupal/languagefield": {
                "#3125977 The language select list widget does not save the selected value": "https://www.drupal.org/files/issues/2020-04-08/issue-3125977-2.patch"
            }
        },
        "enable-patching": true,
        "drupal-scaffold": {
            "locations": {
                "web-root": "web"
            }
        }
    },
    "replace": {
        "drupal/drupal": "*"
    }
}
